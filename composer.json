{
  "name": "i-systems/api",
  "require": {
    "php": "^5.6"
  },
  "require-dev": {
    "behat/behat": "dev-master",
    "phpunit/phpunit": "^5.7",
    "coduo/php-matcher": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "ISklep": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "tests": "./"
    },
    "psr-4": {
      "ISklep\\Behat\\Context\\": "features/bootstrap"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}